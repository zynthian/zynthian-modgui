@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://distrho.sf.net/plugins/MaGigaverb>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-magigaverb.html> ;
        modgui:stylesheet <modgui/stylesheet-magigaverb.css> ;
        modgui:screenshot <modgui/screenshot-magigaverb.png> ;
        modgui:thumbnail <modgui/thumbnail-magigaverb.png> ;
        modgui:brand "DISTRHO" ;
        modgui:label "MaGigaverb" ;
        modgui:model "japanese" ;
        modgui:panel "8-knobs" ;
        modgui:color "cream" ;
        modgui:knob "cream" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "damping" ;
            lv2:name "damping" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "revtime" ;
            lv2:name "revtime" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "roomsize" ;
            lv2:name "roomsize" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "spread" ;
            lv2:name "spread" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "bandwidth" ;
            lv2:name "bandwidth" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "tail" ;
            lv2:name "tail" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "dry" ;
            lv2:name "dry" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "early" ;
            lv2:name "early" ;
        ] ;
    ] .