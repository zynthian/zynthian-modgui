@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://plugin.org.uk/swh-plugins/shaper>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-wave-shaper.html> ;
        modgui:stylesheet <modgui/stylesheet-wave-shaper.css> ;
        modgui:screenshot <modgui/screenshot-wave-shaper.png> ;
        modgui:thumbnail <modgui/thumbnail-wave-shaper.png> ;
        modgui:brand "SWH" ;
        modgui:label "Wave Shaper" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "wood0" ;
        modgui:knob "steel" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "shapep" ;
            lv2:name "Waveshape" ;
        ] ;
    ] .