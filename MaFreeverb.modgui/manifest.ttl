@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://distrho.sf.net/plugins/MaFreeverb>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-mafreeverb.html> ;
        modgui:stylesheet <modgui/stylesheet-mafreeverb.css> ;
        modgui:screenshot <modgui/screenshot-mafreeverb.png> ;
        modgui:thumbnail <modgui/thumbnail-mafreeverb.png> ;
        modgui:brand "DISTRHO" ;
        modgui:label "MaFreeverb" ;
        modgui:model "boxy" ;
        modgui:panel "4-knobs" ;
        modgui:color "cream" ;
        modgui:knob "aluminium" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "fb2" ;
            lv2:name "fb2" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "damp" ;
            lv2:name "damp" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "fb1" ;
            lv2:name "fb1" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "spread" ;
            lv2:name "spread" ;
        ] ;
    ] .