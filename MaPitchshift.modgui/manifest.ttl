@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://distrho.sf.net/plugins/MaPitchshift>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-mapitchshift.html> ;
        modgui:stylesheet <modgui/stylesheet-mapitchshift.css> ;
        modgui:screenshot <modgui/screenshot-mapitchshift.png> ;
        modgui:thumbnail <modgui/thumbnail-mapitchshift.png> ;
        modgui:brand "DISTRHO" ;
        modgui:label "MaPitchshifter" ;
        modgui:model "boxy" ;
        modgui:panel "4-knobs" ;
        modgui:color "petrol" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "blur" ;
            lv2:name "blur" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "window" ;
            lv2:name "window" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "ratio" ;
            lv2:name "ratio" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "xfade" ;
            lv2:name "xfade" ;
        ] ;
    ] .